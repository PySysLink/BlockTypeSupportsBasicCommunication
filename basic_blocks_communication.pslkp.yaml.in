pluginName: basic_blocks_communication
pluginType: coreBlockLibrary
blockType: BasicCommunication
version: @PROJECT_VERSION@
dependsOn:
  - name: BlockTypesBasicCommunication
    version: @PROJECT_VERSION@
blockLibraries:
  - name: BasicBlocksCommunication
    blockTypes:
      - name: OPCUAServer
        configurationValues:
          - name: VariableListFile
            defaultValue: None
            type: string
          - name: OPCUAServerPort
            defaultValue: 4840
            type: int
          - name: InputPortNumber
            defaultValue: 0
            type: int
          - name: OutputPortNumber
            defaultValue: 1
            type: int
      - name: ReadValue
        configurationValues:
          - name: VariableName
            defaultValue: None
            type: string
          - name: VariableType
            defaultValue: Double
            type: string
      - name: WriteValue
        configurationValues:
          - name: VariableName
            defaultValue: None
            type: string
          - name: VariableType
            defaultValue: Double
            type: string
          
      