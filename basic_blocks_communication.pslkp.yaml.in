pluginName: basic_blocks_communication
pluginType: coreBlockLibrary
blockType: BasicCommunication
version: @PROJECT_VERSION@
dependsOn:
  - name: BlockTypesBasicCommunication
    version: @PROJECT_VERSION@
blockLibraries:
  - name: BasicBlocksCommunication
    blockTypes:
      - name: BasicCommunication
        configurationValues:
          - name: CommunicationModule
            defaultValue: ""
            type: string
          - name: CommunicationClass
            defaultValue: ""
            type: string
          - name: InputPortNumber
            defaultValue: 1
            type: int
          - name: OutputPortNumber
            defaultValue: 1
            type: int
          - name: Parameters
            type: string[]
            defaultValue:
            - SampleParameter:1.0
    
      